{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        // "isolatedModules": true,
        "lib": ["es2020"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmit": false,
        // "noFallthroughCasesInSwitch": true,
        // "noUnusedLocals": true,
        // "noUnusedParameters": true,
        "outDir": "out",
        "rootDir": "src",
        "skipLibCheck": true,
        "sourceMap": true,
        // "strict": true,
        "target": "es2020"
    },
    "exclude": ["node_modules", ".vscode-test", "src/wip"]
}
